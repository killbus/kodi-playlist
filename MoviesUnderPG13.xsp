<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Movies Under PG-13</name>
    <match>one</match>
    <rule field="mpaarating" operator="endswith">
        <value>G</value>
    </rule>
    <rule field="mpaarating" operator="endswith">
        <value>Y7</value>
    </rule>
    <rule field="mpaarating" operator="endswith">
        <value>Y</value>
    </rule>
    <order direction="ascending">sorttitle</order>
</smartplaylist>
